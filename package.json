{
	"name": "vscode-terminal-startup-cmd",
	"displayName": "vscode-terminal-startup-cmd",
	"description": "VS Code terminal startup cmd extension",
	"version": "0.0.1",
	"publisher": "serenissimus",
	"license": "BSD 3-Clause License",
	"homepage": "https://github.com/serenissimus/vs-terminal-startup-cmd/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/serenissimus/vs-terminal-startup-cmd.git"
	},
	"engines": {
		"vscode": "^1.33.0"
	},
	"enableProposedApi": true,
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "VS Code terminal startup cmd",
			"properties": {
				"terminal.startupCmd": {
					"type": "string",
					"default": "",
					"markdownDescription": "Cmd which triggered when the terminal is opened",
					"scope": "window"
				}
			}
		}
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"pack": "vsce package",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "tslint -p ./"
	},
	"devDependencies": {
		"@types/node": "^6.0.40",
		"@types/vscode": "^1.33.0",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1",
		"vsce": "^1.73.0"
	}
}
